// vim: ft=c et sw=4

int add_one(int a) {
    return a + 1;
}

int add_two(int a) {
    return a + 2;
}

int f(int c, int d) {
    int a = 0;
    int b = 0;
    for (int i = 0; i < 10; i = i + 1) {
        for (int j = 0; j < 10; j = j + 1) {
            a = add_one(a);
        }
        b = add_two(b);
    }
    // 100 + 20 + 3 + 4
    return a + b + c + d;
}

int main() {
    // 127
    return f(3, 4);
}
